// MAIN STYLESHEET
//
@import nib
@import _mixins
@import _color
@import _typography
@import _strokes


// Bootstrap overrides
html, body
  width: auto !important;
  overflow-x: hidden !important;

body.modal-open
  mr 0px

.btn
  border-radius 0
  border none

.btn-large
  fs 36px

.btn-med
  fs 24px

.btn-primary
  @extend .bg-dblue
  @extend .fw-strong
  fs 1.2em

.btn-info
  @extend .bg-laqua
  @extend .fg-daqua
  @extend .fw-strong
  fs 1.2em

.btn-warning
  @extend .bg-dgrey
  @extend .fw-normal
  &:hover
    @extend .bg-mgrey

.alert.alert-slim
  padding 5px
  mb 0

.top-nav
  mt 15px
  mr 10px
  position relative
  z-index 2

.minimal-back-nav
  border-bottom solid 2px $dblue
  padding 0 15px

.logo-big
  width 213px
  height 211px
  position absolute
  left 50%
  ml -106px
  top 10px
  @media(max-width:767px)
    top 45px

.next-btn
  position absolute
  bottom 40px
  left 40px
  fs 18px
  line-height 5px
  i
    fs 2.5em

.next-btn-fixed
  position fixed
  bottom 20px
  left 20px

.readable-width
  max-width 620px

.readable-width-centered
  max-width 620px
  mr auto
  ml auto

.alt-use
  a
    text-decoration underline
    @extend .fg-dblue

.share-blurb
  mt 12px
  mb 32px
  pr 0
  p
    line-height 2em
  @media(max-width:767px)
    mt 8px

.share-btn
  ml 10px

.share-icon
  @extend .bg-dgrey
  @extend .fg-white
  height 40px
  width 40px
  border-radius 50%
  display block
  text-align center
  line-height 40px
  font-size 20px
  @media(max-width:767px)
    height 32px
    width 32px
    line-height 32px
    font-size 16px

.main-nav
  border none
  border-bottom solid 6px $dblue
  border-radius none
  padding 5px 0 18px 0
  background none
  .navbar-header
    pl 0

.navbar
  border-radius none !important
  @media screen and (min-width: 768px)
    border-radius none !important
  @media(max-width:320px)
    mb 10px

.navbar-brand
  img.little-logo
    display inline
  @media(max-width:767px)
    mb 5px

img.little-logo
  width 56px
  height 56px
  mr 8px
  ml -5px

.force-inline
  @media screen and (min-width: 768px)
    display inline !important

.bb-logotype
  mt 0px
  fs 1.5em
  font-weight bold !important
  color $dblue !important

.section
  padding 80px 0 80px 0
  position relative
  @media(max-width:767px)
    pt 30px
    pb 30px

body
  @extend .bg-white

.splash-section
  pt 0
  pb 0
  .splash-title
    h1
      padding 235px 00 0
      margin 0
      @media(max-width:767px)
        pt 260px
    h4
      mt 5px
      mb 50px

  .btn-primary
    fs 32px
    mt 50px
    mb 50px

  .top-nav.nav-pills
    >li
      >a
        color $dblue
        &:hover
          color $maqua

.wrap-outer-icon
  .wrap-icon
    height 140px
    width 140px
    margin auto
    pt 22px
    border-radius 100px
    border solid 5px #fff

.circ-col
  padding 40px 42px 10px

.little-circ
  border-radius 50px
  position relative
  width 75px
  height 75px
  .circ-label
    margin auto
    position absolute
    top 25px
    left 0
    right 0

.privacy-list
  ml 80px

.big-icon
  fs 600%
  padding 10px

.fa
  display inline-block
  background-repeat no-repeat
  background-position center center
  background-size 100% 100%

// NOTE TO FUTURE FRIENDS: HHS and other gov agencies have browsers that reject icon fonts, hence the conversion to SVG+base64 encoding.

.fa-chevron-up
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-chevron-down
  background-image: url("data:image/svg+xml;base64,"), none;

// .fa-lock
//   background-image: url("data:image/svg+xml;base64,"), none;

.fa-hospital-o
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-mobile
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-file-text-o
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-user-md
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-check-square
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-check-square-dark
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-uncheck-square
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-umbrella
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-medkit
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-flask
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-search
  background-image: url("data:image/svg+xml;base64,"), none;

.fa-random
  background-image: url("data:image/svg+xml;base64,"), none;

.isize-24
  width 24px
  height 24px

.isize-36
  width 36px
  height 36px

.isize-48
  width 48px
  height 48px

.isize-96
  width 96px
  height 96px

.centered-row
  text-align center
  [class*='col-']
    vertical-align top
    float none
    display inline-block

.row.row-keep-margin
  mr 0
  ml 0


.section.why-bb
  @extend .fg-white
  @extend .bg-maqua

  pt 40px
  .why-share, .why-apps
    .wrap-icon
      @extend .bg-daqua
  .why-check
    .wrap-icon
      @extend .bg-daqua
  .why-recall
    .wrap-icon
      @extend .bg-daqua

  h5
    fs 1.1em

.flex-video
  position relative
  pt 0
  pb 67.5%
  height 0
  mb 16px
  overflow hidden
  iframe, object, embed
    position absolute
    top 0
    left 0
    width 100%
    height 100%
  &.vimeo
    pt 0
  &.widescreen
    pb 57.25%

// @media only screen and (max-device-width: 800px), only screen and (device-width: 1024px) and (device-height: 600px), only screen and (width: 1280px) and (orientation: landscape), only screen and (device-width: 800px), only screen and (max-width: 767px)
//   .flex-video
//     pt 0

.vid-still
  position relative

.play-btn-img
  position absolute
  top 20px
  left 0
  right 0
  mr auto
  ml auto

@media screen and (min-width: 768px) and (max-width: 991px)
  #vid-modal
    .modal-dialog
      width 700px

@media screen and (min-width: 992px)
  #vid-modal
    .modal-dialog
      width 800px

  h2.big-header, .lead
    color $maqua
  .img-subtext
    color $dgrey

.section.what-do
  bgc #fff
  pb 50px

.step-num
  fw bold
  fs 28px


.section.testimonials
  bgc $dblue
  color #fff
  @media(max-width:767px)
    pb 80px
  .img-container
    text-align left
    overflow hidden
    padding 0 3px
    margin 4px 0
    position relative
    height 200px
    @media(max-width:767px)
      margin 10px 0 18px
    img.vid-cover
      width 100%
      min-height 200px
    .caption-wrapper
      position absolute
      bottom 0px
      left 0px
      padding 0px 3px
      width 100%
      .caption
        padding 6px
        margin 0
        bgc rgba(0,0,0,0.6)
        color #fff
        width 100%

#what-info
  ul
    li
      mt 4px

.img-subtext
  fw normal
  lh 1.2em
  ta center
  fs 1.5em

.load-profile
  min-height 400px

.feature-list
  li
    i
      mr 10px
    em
      display none
    h6
      color $dgrey

.box-link-rapper
  text-decoration none
  mb 10px
  display table
  // height 150px
  .wrap-source-icon
    bgc $maqua
    height 56px
    width 56px
    border-radius 28px
    mr 10px
    pl 10px
    pt 10px
    &.wrap-pharmacy-icon
      pl 8px

  .data-source-title
    display table-cell
    vertical-align middle

  .pointer-right
    display none
    margin-left 6px

  &:hover, li.active &
    text-decoration none
    .data-source-title
      color $daqua
      &:after
        content " \003E"
        pl 3px
        font-weight bold
    .wrap-source-icon
      bgc $daqua

.use-records-description
  color $maqua
  border-bottom none
  pb 8px

.app-disclaimer
  margin 40px 0

#app-list
  mt 20px
  bgc #eee
  padding 14px

.app-rapper
  mb 40px
  @media(max-width:320px)
    mb 20px

.app-card
  position relative
  height 260px
  @media(max-width:320px)
    height 245px
  pt 10px
  bgc white
  .app-description
    margin 0 5px 0 20px
    p
      mr 15px
      line-height 1.2
      overflow hidden
      position relative
      max-height 6em /*display ellipsis after 3 lines*/
      &:before
        font-weight bold
        font-size 14px
        // text-decoration underline
        // color $daqua
        color $mgrey
        background #fff
        bottom 0
        position absolute
        right 0
        content '\2026'
      &:after
        content ''
        background #fff
        position absolute
        height 50px
        width 100%
        z-index 1
  .app-icon
    // width 128px
    height 64px
  .apple-logo
    width 16px
    mr 8px
  .android-logo
    width 16px
    mr 8px
  .app-reviews
    pl 20px
    width 100%
    position absolute
    bottom 0
    border-top solid 1px $lblue
    pt 6px
    .review
      mb 6px


.nav-pills>li
  >a
    border-radius 0
    color #fff
    fs 18px
    &:hover
      color $dgrey
  &.active>a
    bgc $maqua
    &:hover, &:focus
      bgc $maqua


#select-provider-wrapper
  min-height 250px

.providers
  padding 20px 10px
  // border-top solid 5px $maqua

.provider-list-wrapper
  border-left solid 1px #959dac
  @media(max-width:767px)
    border-left none

.provider-list
  pr 0px
  pb 5px
  height 350px
  overflow-y auto
  &.nav-pills>li>a
    color $mgrey
    fs 16px
    line-height 1.3em
    &.highlight
      bgc #ccc
  @media(max-width:767px)
    height auto

.total-listings
  mt 10px

.stage2-link
  cursor pointer

.provider-contact-details
  pt 6px
  p
    margin-bottom 4px

.no-results
  font-style italic

em.highlight
  bgc #ffd

.provider-search-wrapper
  mb 18px
  mt 5px


.no-data
  mt 0px
  pl 20px
  @media screen and (max-width: 767px)
    border-left none
    pl 0px
    mt 15px
    border-top solid 1px $dgrey

.view-profile-btn
  text-decoration underline

.get-data-btn
  fs 18px

.or
  padding 0 15px

.not-listed-learn-more
  text-decoration underline

.get-records-wrapper-link
  pr 40px
  display inline-block

.provider-about-header, .provider-description
  pr 20px

ul.additional-features-list
  mt 10px
  ml 18px

  li
    line-height 1.5em

.get-records-prep
  @extend .fg-maqua
  position relative
  mt 0
  mb 25px
  img.arrow-up
    position absolute
    top -60px
    right -25px
    z-index 1000
  p
    fs 18px

.go-get-em-header
  fs 18px
  text-transform uppercase
  pt 0
  pb 0

  @media screen and (min-width: 768px) and (max-width: 991px)
    fs 14px

.open-notes-learn-more
  color #999
  ml 5px

p.fs-medium
  mt 10px
  mb 20px

.ask-participate
  mb 40px
  mt 40px
  border-top solid 1px $lgrey

.offerings
  @extend .bg-maqua
  @extend .fg-white
  @extend .clearfix
  padding 0 15px 15px
  margin-left -15px
  margin-right -15px
  ul>li
    fs 16px
    pb 7px

.partners-lists-container
  mb 35px

.app-coming-soon
  fs 5em

.privacy-last-section
  pb 60px

.profile-img
  width 400px

.privacy-health-info-types
  li
    font-weight bold
    mb 25px
    color $dgrey

.scroll-hint-wrapper
  height 30px
  .scroll-hint
    transition opacity 0.5s
    &.no-show
      opacity 0

.section.footer
  pt 20px
  pb 20px
  bgc $lgrey
  p
    mt 8px
    pl 5px
    line-height 1.4em

.footer-links > li
  pb 10px

.pra-title
  margin 15px

.form-group
  mb 25px

.require-message
  padding 6px
